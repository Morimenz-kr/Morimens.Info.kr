        }
        if (skillKit.overexalt) {
            html += `
                <div class="special-skill-card" id="overexalt-card">
                    <h4>${skillKit.overexalt.name} (ê´‘ê¸° ì†Œëª¨: ${skillKit.overexalt.cost_energy})</h4>
