                };

                if (supportInPage && supportInPage.teamIdx !== currentTeamIdx) {
                    openSystemConfirm("ì¡°ë ¥ì ë³€ê²½", "ì´ë¯¸ ë‹¤ë¥¸ íŒŒí‹°ì— ì¡°ë ¥ìê°€ ìˆìŠµë‹ˆë‹¤. í˜„ì¬ íŒŒí‹°ë¡œ ì˜®ê¸°ì‹œê² ìŠµë‹ˆê¹Œ?", applySupport);
                } else {
