        if (card.derives_cards && card.derives_cards.length > 0) {
            card.derives_cards.forEach(derivedId => {
                if (!collectedIds.has(derivedId)) {
                    collectedIds.add(derivedId);
                    collectAllDerivedCards(derivedId, collectedIds);
