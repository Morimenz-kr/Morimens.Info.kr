function renderSidebar() {
    const container = document.getElementById('sidebar-tabs');
    if (!container) return;
    container.innerHTML = '';
    const currentTeams = allPages[currentPageIdx].teams;

    // PCëŠ” ì„¸ë¡œ(80px ë„ˆë¹„), ëª¨ë°”ì¼ì€ ê°€ë¡œ ë°°ì—´ì„ì„ ê°ì§€
    const isVertical = window.innerWidth > 768;
    // ì´ë™í•  ê±°ë¦¬ ê³„ì‚° (ê¸°ì¡´ CSS ìˆ˜ì¹˜ ê¸°ë°˜: ë²„íŠ¼ 44px + ë§ˆì§„ 25px = 69px)
    const shiftDistance = isVertical ? 69 : 50;

    currentTeams.forEach((t, i) => {
        const tab = document.createElement('div');
        // ê¸°ì¡´ í´ë˜ìŠ¤ë“¤(filled, active) 100% ìœ ì§€
        tab.className = `team-tab ${i === currentTeamIdx ? 'active' : ''} ${t.chars.some(x => x) ? 'filled' : ''}`;
        tab.textContent = ROMAN_NUMS[i]; // ê¸°ì¡´ ë¡œë§ˆ ìˆ«ì ì‚¬ìš©
        tab.dataset.index = i;
        tab.draggable = true;

        // ë“œë˜ê·¸ ì‹œì‘ ì‹œì 
        const handleStart = (idx) => {
            tab.classList.add('dragging');
            tab.dataset.fromIdx = idx;
        };

        // ë“œë˜ê·¸ ì¤‘ ì£¼ë³€ ìŠ¬ë¡¯ ë°€ì–´ë‚´ê¸° ë¡œì§ (Unity Layout Group ë°©ì‹)
        const handleMove = (hoverIdx) => {
            const fromIdx = parseInt(tab.dataset.fromIdx);
            const allTabs = document.querySelectorAll('.team-tab');

            allTabs.forEach((el, idx) => {
                if (idx === fromIdx) return; // ë“œë˜ê·¸ ì¤‘ì¸ ë³¸ì²´ëŠ” ì œì™¸

                const moveValue = (isVertical ? `translateY` : `translateX`);

                if (fromIdx < hoverIdx) { // ì•„ë˜(ì˜¤ë¥¸ìª½)ë¡œ ë“œë˜ê·¸ ì‹œ
                    if (idx > fromIdx && idx <= hoverIdx) {
                        el.style.transform = `${moveValue}(-${shiftDistance}px)`;
                    } else {
                        el.style.transform = '';
                    }
                } else if (fromIdx > hoverIdx) { // ìœ„(ì™¼ìª½)ë¡œ ë“œë˜ê·¸ ì‹œ
                    if (idx < fromIdx && idx >= hoverIdx) {
                        el.style.transform = `${moveValue}(${shiftDistance}px)`;
                    } else {
                        el.style.transform = '';
                    }
                } else {
                    el.style.transform = '';
                }
            });
        };

        const handleEnd = (targetIdx) => {
            tab.classList.remove('dragging');
            document.querySelectorAll('.team-tab').forEach(el => el.style.transform = '');

            const fromIdx = parseInt(tab.dataset.fromIdx);
            if (!isNaN(targetIdx) && fromIdx !== targetIdx) {
                executeSwap(fromIdx, targetIdx); // ì‹¤ì œ ë°ì´í„° êµì²´
            }
        };

        // PC ì´ë²¤íŠ¸ ë°”ì¸ë”©
        tab.ondragstart = () => handleStart(i);
        tab.ondragover = (e) => { e.preventDefault(); handleMove(i); };
        tab.ondrop = () => handleEnd(i);
        tab.ondragend = () => handleEnd();

        // ëª¨ë°”ì¼ í„°ì¹˜ ì´ë²¤íŠ¸ ë°”ì¸ë”© (elementFromPointë¡œ íƒ€ê²Ÿ ê°ì§€)
        tab.ontouchstart = () => handleStart(i);
        tab.ontouchmove = (e) => {
            const touch = e.touches[0];
            const target = document.elementFromPoint(touch.clientX, touch.clientY);
            if (target && target.classList.contains('team-tab')) {
                const hIdx = parseInt(target.dataset.index);
                handleMove(hIdx);
                tab.dataset.lastHoverIdx = hIdx;
            }
        };
        tab.ontouchend = () => handleEnd(parseInt(tab.dataset.lastHoverIdx));

        tab.onclick = () => { currentTeamIdx = i; renderAll(); };
        container.appendChild(tab);
    });
}

function executeSwap(fromIdx, toIdx) {
    if (fromIdx === toIdx || isNaN(fromIdx)) return;

    const teams = allPages[currentPageIdx].teams;
    const [draggedItem] = teams.splice(fromIdx, 1);
    teams.splice(toIdx, 0, draggedItem);

    // ì¸ë±ìŠ¤ íŠ¸ë˜í‚¹ (ìœ ì €ê°€ ë³´ê³  ìˆë˜ í˜ì´ì§€ ìœ ì§€)
    if (currentTeamIdx === fromIdx) {
        currentTeamIdx = toIdx;
    } else if (fromIdx < currentTeamIdx && toIdx >= currentTeamIdx) {
        currentTeamIdx--;
    } else if (fromIdx > currentTeamIdx && toIdx <= currentTeamIdx) {
        currentTeamIdx++;
    }

    renderAll();
    saveAllData(true);
}

