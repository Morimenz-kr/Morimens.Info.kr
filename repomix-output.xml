    --sidebar-bg: #141414;
    --card-bg: #252529;
    --highlight: #f7a115;
    --slot-border: #555;
    --text-main: #e6e6e6;
    /* ëª¨ë‹¬ ë””ìì¸ í‘œì¤€ ê·œê²© */
    --modal-bg: #222;
