/* (ê¸°ë³¸ CSS) */
body { font-family: Arial, sans-serif; margin: 0; padding: 20px; background-color: #333; color: #eee; }
.container { max-width: 1200px; margin: 20px auto; background-color: #444; padding: 20px; border-radius: 10px; }
.back-button { display: inline-block; padding: 8px 15px; background-color: #6c757d; color: #fff; text-decoration: none; border-radius: 5px; margin-bottom: 20px; }
.back-button:hover { background-color: #5a6268; }
.detail-layout { display: flex; gap: 30px; }
.char-profile { flex-basis: 35%; text-align: center; }
.char-profile img { width: 100%; border-radius: 10px; border: 3px solid #555; margin-bottom: 15px; }
.char-profile h2 { margin: 0 0 10px 0; font-size: 2.5em; }
.char-profile .tags { display: flex; justify-content: center; align-items: center; gap: 10px; margin-bottom: 20px; }
.char-profile .tag { padding: 5px 12px; border-radius: 15px; font-weight: bold; color: #fff; }
.tag.ssr { background-color: #ffc107; color: #333; }
.tag.sr { background-color: #8a2be2; }
.tag.chaos { background-color: #8B8000; }
.tag.aequor { background-color: #005A8C; }
.tag.caro { background-color: #8C0000; }
.tag.ultra { background-color: #5A008C; }
.tag.assault { background-color: #dc3545; }
.tag.warden { background-color: #007bff; }
.tag.chorus { background-color: #28a745; }
.char-profile .lore { text-align: left; background-color: #333; padding: 15px; border-radius: 5px; font-size: 0.9em; color: #ccc; }
.char-data { flex-basis: 65%; }
.data-tabs { display: flex; border-bottom: 2px solid #666; margin-bottom: 20px; }
.data-tabs .tab-link { padding: 10px 20px; cursor: pointer; border: none; background-color: transparent; color: #aaa; font-size: 1.1em; font-weight: bold; }
.data-tabs .tab-link.active { color: #fff; border-bottom: 3px solid #ffc107; }
.tab-content { display: none; }
.tab-content.active { display: block; }
h4 { color: #ffc107; border-bottom: 1px solid #555; padding-bottom: 5px; }

