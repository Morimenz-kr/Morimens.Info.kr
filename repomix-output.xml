            else if (usedInOtherTeamsNormal.has(id)) conflictReason = "ì‚¬ìš©ì¤‘";
            else if (activeDomains.size >= 2 && !activeDomains.has(c.relems)) conflictReason = "ì˜ì—­ ì¶©ëŒ";
        } else {
            // [ì¼ë°˜ ëŒ€ì› í¸ì„± ëª¨ë“œ]
