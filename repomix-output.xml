                if (description) {

                    tooltipBox.innerHTML = description;

                    // ìœ„ì¹˜ ê³„ì‚° (DOM ê³„ì‚°ì„ ìœ„í•´ ì ì‹œ í‘œì‹œ)
                    tooltipBox.style.visibility = 'visible';
