    function setupTooltipListeners() {
        // íˆ´íŒ ë°•ìŠ¤ê°€ ì—†ìœ¼ë©´ ìƒì„±í•˜ì—¬ bodyì— ì¶”ê°€
        let tooltipBox = document.getElementById('global-tooltip-box');
        if (!tooltipBox) {
            tooltipBox = document.createElement('div');
            tooltipBox.id = 'global-tooltip-box';
