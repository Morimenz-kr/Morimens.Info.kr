/* --- [ëª¨ë‹¬ ê³µí†µ] --- */
.modal-overlay {
    display: none; position: fixed; top: 0; left: 0; width: 100%; height: 100%;
    background: rgba(0,0,0,0.85); z-index: 1000; justify-content: center; align-items: center;
}
.modal-overlay.show { display: flex; }

.modal-box {
    width: 90%; max-width: 900px; height: 85vh;
    background: var(--modal-bg);
    border: 1px solid var(--modal-border);
    border-radius: var(--modal-radius);
    display: flex; flex-direction: column;
    overflow: hidden;
}

.modal-box-wide {
    width: 95%; max-width: 1400px; height: 90vh;
    background: var(--modal-bg);
    border: 1px solid var(--modal-border);
    border-radius: var(--modal-radius);
    display: flex; flex-direction: row;
    overflow: hidden;
}

.modal-header {
    padding: 15px 20px;
    background: transparent;
    border-bottom: 1px solid var(--modal-border);
    display: flex; justify-content: space-between; align-items: center;
}

.modal-header h3 {
    margin: 0;
    color: var(--modal-accent);
    font-size: 1.2rem;
    font-weight: bold;
}
.close-btn { background: none; border: none; color: #999; font-size: 1.5em; cursor: pointer; }
/* íˆ´íŒ ë‚´ ì£¼ì˜µì…˜ ê°•ì¡° ìŠ¤íƒ€ì¼ */
.tooltip-main-stat {
    color: #ffc107; /* ê°•ì¡°ìƒ‰ (ë…¸ë€ìƒ‰) */
    font-weight: bold;
    font-size: 1.05em;
    padding: 6px 8px;
    background: rgba(255, 193, 7, 0.1);
    border-radius: 4px;
    margin-bottom: 10px;
