    --modal-border: #444;
    --modal-accent: #ffc107;
}

body {
    background-color: var(--bg-dark);
    color: var(--text-main);
    overflow: hidden;
    font-family: 'Pretendard', sans-serif;
    margin: 0; padding: 0;
}

.builder-container {
