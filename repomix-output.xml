            document.body.appendChild(tooltipBox);
        }

        // ë§ˆìš°ìŠ¤ ì˜¤ë²„ ì´ë²¤íŠ¸ ìœ„ì„
        document.addEventListener('mouseover', (e) => {
            if (e.target.matches('.tooltip-trigger')) {
                const keyword = e.target.dataset.keyword;
